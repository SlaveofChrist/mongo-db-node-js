{
    "version": 2,
    "builds": [
        {
            "src": "server.py",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "git": {
        "deploymentEnabled": {
            "main": false
        }
    }
}